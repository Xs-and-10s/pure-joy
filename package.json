  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "testOld": "node scripts/test.js --env=jsdom",
    "watch": "npm run pack -- --watch",
    "webpack": "webpack --module-bind='js=babel' src/index.js public/bundle.js",
    "lint": "eslint src/**/*.js webpack.config.js",
    "pack": "webpack",
    "dev": "webpack-dev-server",
    "test": "NODE_ENV=test jest",
    "update-test": "npm run test -- -u",
    "cover": "npm run test -- --coverage",
    "server-start": "NODE_ENV=server node server.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
